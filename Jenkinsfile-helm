pipeline {
    environment {
        helm_back_1="back service 1"
        helm_front="front service"
    }
    agent any

    stages {
        stage('TESTING RESULTS') {
            steps {
                script {
                    def folders = sh(returnStdout: true, script: "git show | grep -E ^--- | cut -d / -f2")
                    folders.each { item ->
                        sh "echo Hello ${item}"
                    }
                }
            }
        }
      
    }
}
