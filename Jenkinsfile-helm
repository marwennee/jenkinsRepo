pipeline {
    environment {
        helm_back_1="back service 1"
        helm_front="front service"
    }
    agent any

    stages {
        stage('TESTING RESULTS') {
            steps {
                script {
                    String folders_separatedBy_space = sh(returnStdout: true, script: "git show | grep -o b/.*/ | cut -d / -f2 | uniq | tr '\n' ' '")
                    String[] folderList = title.split(" ");
                    folderList.each { folder ->
                        sh "echo Hello ${folder}"
                    }
                }
            }
        }
      
    }
}
